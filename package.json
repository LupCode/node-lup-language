{
  "name": "lup-language",
  "version": "2.2.2",
  "description": "Node express middleware for detecting requested language",
  "files": [
    "lib/**/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepublishOnly": "npm run lint && npm run build && npm test",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LupCode/node-lup-language.git"
  },
  "keywords": [
    "Express",
    "Middleware",
    "multi-language",
    "multi-lingual",
    "NextJS",
    "Next.js"
  ],
  "author": "LupCode.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LupCode/node-lup-language/issues"
  },
  "homepage": "https://github.com/LupCode/node-lup-language#readme",
  "dependencies": {
    "lup-root": "^1.3.10"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.8.3"
  }
}
